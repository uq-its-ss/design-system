    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "glob": "^7.2.0",
    "mini-svg-data-uri": "^1.4.3",
    "prettier": "^2.5.1",
    "svgdom": "^0.1.10",
