{
  "name": "@uqds/icon",
  "version": "4.0.0",
  "description": "Icon component",
  "keywords": [
    "uqds",
    "uq",
    "University of Queensland",
    "design system",
    "scss",
    "sass",
    "css",
    "icon"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "University of Queensland",
    "email": "webservices@uq.edu.au",
    "url": "https://uq.edu.au"
  },
  "homepage": "https://github.com/uq-its-ss/design-system#readme",
  "license": "ISC",
  "main": "index.js",
  "directories": {
    "lib": "src"
  },
  "files": [
    "index.js",
    "dist/*",
    "src/js/*",
    "src/scss/*",
    "src/ts/_build/*"
  ],
  "style": "dist/css/main.css",
  "types": "src/ts/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uq-its-ss/design-system.git"
  },
  "scripts": {
    "build": "npm run clean-icons && npm run build-icons && npm run format",
    "build-icons": "tsx src/ts/build-icons/index.ts",
    "clean": "npm run clean-icons",
    "clean-icons": "rm -rf ./dist/images src/*/_build/*",
    "format": "prettier --write \"**/*.{css,scss,js,jsx,json,md,mdx,tsx}\"",
    "prepare": "gulp -f ../../gulpfile.js --cwd=. prepare"
  },
  "bugs": {
    "url": "https://github.com/uq-its-ss/design-system/issues"
  },
  "dependencies": {
    "@uqds/core": "^4.0.0"
  },
  "devDependencies": {
    "@svgdotjs/svg.js": "^3.1.2",
    "@types/glob": "^7.2.0",
    "@types/node": "^17.0.16",
    "@types/svgo": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^8.57.1",
    "glob": "^7.2.0",
    "gulp": "^5.0.0",
    "mini-svg-data-uri": "^1.4.3",
    "prettier": "^2.5.1",
    "svgdom": "0.1.14",
    "svgo": "^2.8.0",
    "tsx": "^4.20.3",
    "typescript": "^4.5.5"
  }
}
