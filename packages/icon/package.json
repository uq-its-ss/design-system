{
  "name": "@uqds/icon",
  "version": "0.0.19-alpha.0",
  "description": "Icon component",
  "keywords": [
    "uqds",
    "uq",
    "University of Queensland",
    "design system",
    "scss",
    "sass",
    "css",
    "icon"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "University of Queensland",
    "email": "webservices@uq.edu.au",
    "url": "https://uq.edu.au"
  },
  "homepage": "https://github.com/uq-its-ss/design-system#readme",
  "license": "ISC",
  "main": "index.js",
  "directories": {
    "lib": "src"
  },
  "files": [
    "index.js",
    "dist/*",
    "src/js/*",
    "src/scss/*"
  ],
  "style": "dist/css/main.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uq-its-ss/design-system.git"
  },
  "scripts": {
    "build": "npm run build-ts && npm run build-icons",
    "build-ts": "tsc -p ./src/ts/tsconfig.json",
    "build-icons": "node dist/index.js",
    "clean": "npm run clean-ts && npm run clean-icons",
    "clean-ts": "rm -rf ./dist/*.js*",
    "clean-icons": "rm -rf ./dist/images src/*/_build/*",
    "prepare": "gulp -f ../../gulpfile.js --cwd=. prepare"
  },
  "bugs": {
    "url": "https://github.com/uq-its-ss/design-system/issues"
  },
  "dependencies": {
    "@uqds/core": "^1.2.4-alpha.0"
  },
  "devDependencies": {
    "@svgdotjs/svg.js": "^3.2.4",
    "@types/glob": "^8.1.0",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^8.57.1",
    "glob": "^7.2.0",
    "gulp": "^5.0.0",
    "mini-svg-data-uri": "^1.4.4",
    "prettier": "^3.3.3",
    "svgdom": "0.1.19",
    "svgo": "^3.3.2",
    "typescript": "^5.6.3"
  }
}
