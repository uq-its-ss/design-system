{
  "name": "@uqds/react",
  "version": "2.0.5",
  "description": "React components",
  "keywords": [
    "uqds",
    "uq",
    "University of Queensland",
    "design system",
    "scss",
    "sass",
    "css",
    "accordion",
    "react"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "University of Queensland",
    "email": "webservices@uq.edu.au",
    "url": "https://uq.edu.au"
  },
  "homepage": "https://github.com/uq-its-ss/design-system#readme",
  "license": "ISC",
  "type": "module",
  "directories": {
    "lib": "src"
  },
  "files": [
    "src/*",
    "dist/*"
  ],
  "main": "dist/modules/index.js",
  "types": "./dist/modules/index.d.ts",
  "module": "dist/modules/index.js",
  "exports": {
    ".": {
      "types": "./dist/modules/index.d.ts",
      "import": "./dist/modules/index.js"
    },
    "./plain": {
      "types": "./dist/plain/index.d.ts",
      "import": "./dist/plain/index.js"
    },
    "./css": {
      "types": "./src/types/css.d.ts",
      "default": "./dist/modules/index.css"
    },
    "./plain/css": {
      "types": "./src/types/css.d.ts",
      "default": "./dist/plain/index.css"
    },
    "./scss": "./src/scss/main.scss",
    "./tsx": "./src/jsx/index.ts"
  },
  "style": "dist/modules/index.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uq-its-ss/design-system.git"
  },
  "scripts": {
    "prepare": "gulp -f ../../gulpfile.js --cwd=. prepare && npm run build",
    "build": "npm run build:modules && npm run build:plain",
    "build:modules": "vite build --config vite.config.modules.js",
    "build:plain": "vite build --config vite.config.plain.js",
    "lint-tsc": "tsc"
  },
  "bugs": {
    "url": "https://github.com/uq-its-ss/design-system/issues"
  },
  "dependencies": {
    "@uqds/alert": "^7.0.6",
    "@uqds/body": "^4.0.2",
    "@uqds/button": "^4.0.0",
    "@uqds/card": "^4.0.1",
    "@uqds/core": "^5.0.0",
    "@uqds/hero": "^7.0.0",
    "@uqds/icon": "^4.0.3",
    "@uqds/layout": "^7.0.0",
    "@uqds/loading-spinner": "^4.0.3",
    "@uqds/pagination": "^4.0.1",
    "@uqds/pane": "^4.0.0",
    "@uqds/table": "^4.0.0",
    "classnames": "^2.5.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@vitejs/plugin-react": "^4.6.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-commonjs": "^0.10.4",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  }
}
