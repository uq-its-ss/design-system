{
  "core": {
    "workingDir": ".reg",
    "actualDir": "screenshots",
    "thresholdRate": 0,
    "addIgnore": true,
    "ximgdiff": {
      "invocationType": "client"
    }
  },
  "plugins": {
    "reg-notify-github-with-api-plugin": {
      "githubUrl": "https://api.github.com/graphql",
      "owner": "uq-its-ss",
      "repository": "design-system",
      "privateToken": "$UQDS_PERSONAL_ACCESS_TOKEN"
    },
    "reg-publish-s3-plugin": {
      "bucketName": "$AWS_S3_BUCKET_TOOLS"
    },
    "reg-keygen-git-hash-plugin": {}
  }
}
